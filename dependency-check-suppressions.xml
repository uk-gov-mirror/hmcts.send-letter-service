<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">

    <!-- PGP! -->

    <suppress>
        <notes><![CDATA[
            File name: bcpg-jdk15on-1.60.jar
            No newer version available.
        ]]></notes>
        <gav regex="true">^org\.bouncycastle:bcpg-jdk15on:1\.60$</gav>
        <cve>CVE-2001-0381</cve>
        <cve>CVE-2005-0366</cve>
    </suppress>

    <!-- not used by server -->

    <suppress>
        <notes><![CDATA[Used only for testing not used in prod]]></notes>
        <gav regex="true">^org\.testcontainers:postgresql:.*$</gav>
        <cpe>cpe:/a:postgresql:postgresql</cpe>
    </suppress>
    <suppress>
        <notes><![CDATA[Related to Groovy LDAP API which is not used by the service]]></notes>
        <gav regex="true">^org\.codehaus\.groovy:groovy-xml:.*$</gav>
        <cpe>cpe:/a:apache:groovy</cpe>
    </suppress>
    <suppress>
        <notes><![CDATA[Related to Groovy LDAP API which is not used by the service]]></notes>
        <gav regex="true">^org\.codehaus\.groovy:groovy-json:.*$</gav>
        <cve>CVE-2016-6497</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[
   Need to update web jar version once available .
   d3.js was a malicious module published with the intent to hijack environment variables
   ]]></notes>
        <gav regex="true">^org\.webjars:d3js:.*$</gav>
        <cpe>cpe:/a:d3.js_project:d3.js</cpe>
    </suppress>
</suppressions>
